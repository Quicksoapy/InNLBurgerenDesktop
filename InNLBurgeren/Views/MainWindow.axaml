<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:vm="using:InNLBurgeren.ViewModels"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        Width="800" Height="450"
        x:CompileBindings="True"
        x:DataType="vm:MainWindowViewModel"
        x:Class="InNLBurgeren.Views.MainWindow"
        Icon="/Assets/inNLburgeren_ROUNDED_off.ico"
        
        Title="InNLBurgeren">
        
        <Grid>

                <ListBox>
                        <Label>Username: </Label>
                        <TextBox Width="100" x:Name="Username" Margin="0,0,670,0"></TextBox>
                        <Label>Password: </Label>
                        <TextBox Width="100" x:Name="Password" Margin="0,0,670,0"></TextBox>
                        <Button Command="{Binding LoginEventHandler}" Width="65" Height="30">Submit</Button>
                </ListBox>
                
        </Grid>
        
</Window>
